<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="qctracker_sub_process_view_tree" model="ir.ui.view">
        <field name="name">qctracker.sub.process.tree</field>
        <field name="model">qctracker.sub.process</field>
        <field name="arch" type="xml">
            <tree class="qctracker-sub-process-tree">
                <field name="name" />
                <field name="process_id" />
            </tree>
        </field>
    </record>

    <record id="qctracker_sub_process_view_form" model="ir.ui.view">
        <field name="name">qctracker.sub.process.form</field>
        <field name="model">qctracker.sub.process</field>
        <field name="arch" type="xml">
            <form class="qctracker-sub-process-form">
                <sheet>
                    <group>
                        <field name="name" class="animated pulse" />
                        <field name="process_id" class="animated pulse" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>