<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_qctracker_process_importer_form" model="ir.ui.view">
        <field name="name">qctracker.process.importer.form</field>
        <field name="model">qctracker.process.importer</field>
        <field name="arch" type="xml">
            <form string="Importer des Projets de Processus">
                <group>
                    <field name="file" filename="file_name"/>
                    <field name="file_name" invisible="1"/>
                </group>
                <footer>
                    <button name="action_import_uploaded_file" type="object" string="Importer" class="oe_highlight"/>
                    <button special="cancel" string="Annuler"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>